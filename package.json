{
  "name": "rgd-bot",
  "version": "1.0.0",
  "license": "ISC",
  "author": "RGD Community",
  "description": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Russian-Gamedev/rgd-bot.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/Russian-Gamedev/rgd-bot#issues"
  },
  "homepage": "https://github.com/Russian-Gamedev/rgd-bot#readme",
  "scripts": {
    "start": "node ./build/index.js",
    "start:dev": "ts-node ./src/index.ts",
    "format": "prettier --write \"src/**/*.ts\" ",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "ts-check": "tsc --noEmit --incremental",
    "prepare": "husky install .github/.husky",
    "build": "tsc -p . && tsc-alias -p tsconfig.json",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org damirlutdev --project rgd-bot ./build && sentry-cli sourcemaps upload --org damirlutdev --project rgd-bot ./build"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint"
    ]
  },
  "dependencies": {
    "@sapphire/discord-utilities": "3.2.1",
    "@sapphire/framework": "5.0.4",
    "@sapphire/pieces": "4.1.0",
    "@sapphire/plugin-api": "6.0.1",
    "@sapphire/plugin-scheduled-tasks": "9.0.1",
    "@sapphire/plugin-subcommands": "6.0.1",
    "@sapphire/result": "2.6.5",
    "@sapphire/time-utilities": "1.7.11",
    "@sapphire/utilities": "3.14.0",
    "@sentry/node": "7.88.0",
    "@sentry/profiling-node": "1.3.2",
    "discord.js": "14.14.1",
    "dotenv": "16.3.1",
    "node-cron": "3.0.3",
    "pg": "8.11.3",
    "reflect-metadata": "0.2.1",
    "tslib": "2.6.2",
    "typeorm": "0.3.17"
  },
  "devDependencies": {
    "@commitlint/cli": "18.4.3",
    "@commitlint/config-conventional": "18.4.3",
    "@sapphire/decorators": "6.0.3",
    "@sentry/cli": "2.23.0",
    "@types/node": "20.10.4",
    "@types/node-cron": "3.0.11",
    "@typescript-eslint/eslint-plugin": "6.14.0",
    "@typescript-eslint/parser": "6.14.0",
    "cross-env": "7.0.3",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "husky": "8.0.3",
    "lint-staged": "15.2.0",
    "nodemon": "3.0.2",
    "prettier": "3.1.1",
    "semantic-commit-emoji": "0.6.2",
    "ts-node": "10.9.2",
    "tsc-alias": "1.8.8",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.3.3"
  }
}
